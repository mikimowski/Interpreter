### Functions and Arrays ###

## Example1

int f([int] arr) {
    arr[0] = arr[0] + 1;
    return 0;
}

[int] arr3 = [1, 2, 3];
f(arr3);
# arr3 == [1, 2, 3]
println(arr3);

f(&arr3);
# arr3 == [2, 2, 3]
println(arr3);



## Example 2

[int] f2([int] arr) {
    arr[0] = arr[0] + 1;
    return arr;
}

[int] arr = [1, 2, 3];
[int] res = f2(arr);
# arr == [1, 2, 3]
# res == [2, 2, 3]

println(arr);
println(res);
